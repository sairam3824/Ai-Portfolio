{
    "buildCommand": "npm run build",
    "outputDirectory": "Version2/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/v1/:path*",
            "destination": "/v1/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}